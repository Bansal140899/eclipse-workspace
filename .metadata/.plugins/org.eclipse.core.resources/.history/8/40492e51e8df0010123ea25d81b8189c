package com.ajaysoft.a;


class Node {
    int data;
    Node next;

    Node(int data) {
        this.data = data;
        this.next = null;
    }
}


class MyLinkedList {

    Node head;
    Node tail;

   
    void insert_first(int data) {

        Node newNode = new Node(data);

        if (head == null) {
            head = newNode;
            tail = newNode;
            return;
        }

        head.next = head;
        head = newNode;
    }

    
    void insert_last(int data) {

        Node newNode = new Node(data);

        if (head == null) {
            head = newNode;
            tail = newNode;
            return;
        }

        tail.next = newNode;
        tail = newNode;
    }
    
    void delete_first() {

        if (head == null) {
            System.out.println("List is empty");
            return;
        }

        head = head.next;

        
        if (head == null) {
            tail = null;
        }
    }
//    void insert_delte(int data) {
//
//        Node newNode = new Node(data);
//
//        if (head != null) {
//            head = newNode;
//            tail = newNode;
//            return;
//        }
//
//        tail.next = newNode;
//        tail = newNode;
//    }

    
    void printList() {
        Node temp = head;

        while (temp != null) {
            System.out.print(temp.data + " ");
            temp = temp.next;
        }
        System.out.println();
    }
}


public class Linked_List {
    public static void main(String[] args) {

        MyLinkedList l1 = new MyLinkedList();

        l1.insert_last(10);
        l1.delete_first();
        l1.insert_last(20);
        l1.insert_last(30);

        l1.insert_first(5);
        

        l1.printList();
    }
}
