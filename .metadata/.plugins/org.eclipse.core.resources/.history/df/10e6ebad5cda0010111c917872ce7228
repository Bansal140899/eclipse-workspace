
public class Rotatethearray {
	
	
	public static void main(String[] args) {
		
		int num [] = {1 ,2 ,3 ,4 ,5 ,6,7};
		
		for(int i =0; i<=num.length/2; i++) {
			int temp = num[num.length-1-i];
			num[num.length-1-i] = num[i];
			num[i] = temp;
			
			
		}
		
		for(int i :num) {
			System.out.println(i);
		}
		
	}

}
