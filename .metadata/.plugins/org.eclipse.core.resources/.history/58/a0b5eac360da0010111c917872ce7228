
public class Rotatethearray {
	  
//	public static int[] rotate(int num[] ,int start ,int end) {
//		
//	}
	
	
	public static void main(String[] args) {
		
		int num [] = {1 ,2 ,3 ,4 ,5 ,6,7};
		int k = 4;
		int i =0;
		int j =num.length-1;
	 
		
		while(i<j) {
			int temp = num[i];
			num[i] = num[j];
			num[j]=temp;
			i++;
			j--;
			
			
		}
		
		for(int l : num) {
			System.out.println(l);
		}
	}

}
